<%= form_for @budget do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <br></br>

  <%= f.label :limit %>
  <%= f.text_field :limit %>

  <br></br>
  <%= f.label :start_date %>
  <%= f.date_field :start_date %>
  <br></br>
  <%= f.label :end_date %>
  <%= f.date_field :end_date %>
  <br></br>
  <%= f.submit %>

<% end %>
